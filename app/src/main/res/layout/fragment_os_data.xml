                    android:textColor="@color/text_color_os_item"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/text_view_os_designacao_descricao"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/os_designacao_descricao"
                    android:layout_marginLeft="8dp"
                    android:layout_marginTop="2.5dp"
                    android:text="Host 33973: BERNADETE ESTAVAO DOS SANTOS"
                    android:textColor="@color/text_color_os_item"
                    android:textSize="16sp" />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/os_layout_rede"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/os_rede"
                    fontPath="fonts/Lato-Bold.ttf"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Rede:"
                    android:textColor="@color/text_color_os_item"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/text_view_os_rede"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/os_rede"
                    android:layout_marginLeft="8dp"
                    android:layout_marginTop="2.5dp"
                    android:text="VALENET_ETCETCETC"
                    android:textColor="@color/text_color_os_item"
                    android:textSize="16sp" />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/os_layout_status"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/os_status"
                    fontPath="fonts/Lato-Bold.ttf"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
